programa {
  funcao inicio() {
    //declaração de variavel, 
    real valorA, valorB

//atribuição
escreva ("digite o valor de A: ")
leia(valorA)
escreva("digite o valor de B:")
leia(valorB)
//operações aritimetricas
escreva("\n soma de A e B",(valorA + valorB))
escreva("\n subtração de A e B",(valorA - valorB))
escreva("\n multiplicação de A e B",(valorA * valorB))
escreva("\n divisão de A e B",(valorA / valorB))
escreva("\n resto de A e B",(valorA % valorB))

  }
}
